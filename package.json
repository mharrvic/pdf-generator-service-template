{
  "name": "pdf-service",
  "version": "1.0.0",
  "description": "PDF Generator Service using React-PDF and Terraform for infrastructure automation.",
  "main": "index.js",
  "scripts": {
    "preview": "vite",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec './node_modules/.bin/ts-node' src/index.ts",
    "serve:build": "cd build && node index.js",
    "build": "rimraf build && esbuild src/index.ts --platform=node  --bundle --minify --outfile=build/index.js"
  },
  "keywords": [
    "pdf generator",
    "react-pdf",
    "react components to pdf",
    "pdf generator service"
  ],
  "author": "mharrvic@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^6.8.0",
    "@react-pdf/renderer": "^3.0.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.10",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  }
}
