{
  "name": "pdf-service",
  "version": "1.0.0",
  "description": "PDF Generator Service using React-PDF",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec './node_modules/.bin/ts-node' src/index.ts",
    "serve:build": "cd build && node index.js",
    "build": "rimraf build && esbuild src/index.ts --platform=node  --bundle --minify --outfile=build/index.js"
  },
  "keywords": [
    "pdf generator",
    "react-pdf",
    "react components to pdf",
    "pdf generator service"
  ],
  "author": "mharrvic@gmail.com",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.10",
    "express": "^4.18.2",
    "rimraf": "^3.0.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
